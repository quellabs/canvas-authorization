{
  "name": "quellabs/canvas-authorization",
  "description": "",
  "keywords": [
    "canvas-framework"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/quellabs/canvas-authorization",
  "support": {
    "issues": "https://github.com/quellabs/canvas-authorization/issues",
    "source": "https://github.com/quellabs/canvas-authorization"
  },
  "authors": [
    {
      "name": "Quellabs",
      "email": "support@quellabs.com",
      "homepage": "https://www.canvasphp.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "8.2"
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "classmap-authoritative": true
  },
  "require": {
    "quellabs/contracts": "^1.0",
    "quellabs/sculpt": "^1.0",
    "quellabs/canvas-validation": "dev-feature/reimplement-rules"
  },
  "autoload": {
    "psr-4": {
      "Quellabs\\CanvasAuthorization\\": "src/"
    }
  },
  "extra": {
    "discover": {
      "publishers": {
        "provider": "Quellabs\\CanvasAuthorization\\Publishing\\AuthorizationPublisher"
      }
    }
  }
}